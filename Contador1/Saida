#! /usr/local/bin/vvp
:ivl_version "0.10.0 (devel)" "(s20150513)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x89a2b48 .scope module, "Teste" "Teste" 2 1;
 .timescale 0 0;
v0x899f420_0 .var "clk", 0 0;
v0x899f498_0 .var "rst", 0 0;
v0x899f510_0 .net "s", 4 0, v0x899f368_0;  1 drivers
S_0x897f288 .scope module, "c1" "Contador" 2 17, 3 1 0, S_0x89a2b48;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /OUTPUT 5 "s"
v0x897f3c8_0 .net "clk", 0 0, v0x899f420_0;  1 drivers
v0x899f290_0 .var "direction", 0 0;
v0x899f2f8_0 .net "rst", 0 0, v0x899f498_0;  1 drivers
v0x899f368_0 .var "s", 4 0;
E_0x897f3a0 .event posedge, v0x899f2f8_0, v0x897f3c8_0;
    .scope S_0x897f288;
T_0 ;
    %wait E_0x897f3a0;
    %load/vec4 v0x899f2f8_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x899f368_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x899f290_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x899f290_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x899f368_0;
    %subi 1, 0, 5;
    %store/vec4 v0x899f368_0, 0, 5;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x899f368_0;
    %addi 1, 0, 5;
    %store/vec4 v0x899f368_0, 0, 5;
T_0.3 ;
    %load/vec4 v0x899f368_0;
    %cmpi/e 15, 0, 5;
    %flag_mov 8, 4;
    %load/vec4 v0x899f368_0;
    %cmpi/e 0, 0, 5;
    %flag_or 4, 8;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x899f290_0;
    %inv;
    %store/vec4 v0x899f290_0, 0, 1;
T_0.4 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x89a2b48;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x899f420_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x899f498_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x899f498_0, 0, 1;
    %pushi/vec4 100, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %load/vec4 v0x899f420_0;
    %inv;
    %store/vec4 v0x899f420_0, 0, 1;
    %delay 10, 0;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %end;
    .thread T_1;
    .scope S_0x89a2b48;
T_2 ;
    %vpi_call/w 2 19 "$monitor", v0x899f510_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
