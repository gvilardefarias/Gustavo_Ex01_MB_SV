#! /usr/local/bin/vvp
:ivl_version "0.10.0 (devel)" "(s20150513)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x9f9f498 .scope module, "Teste" "Teste" 2 1;
 .timescale 0 0;
v0x9f9bb68_0 .var "clk", 0 0;
v0x9f9bbe0_0 .var "rst", 0 0;
v0x9f9bc58_0 .net "s", 3 0, v0x9f9bab0_0;  1 drivers
S_0x9f7b918 .scope module, "c1" "Contador" 2 23, 3 1 0, S_0x9f9f498;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /OUTPUT 4 "s"
v0x9f7ba58_0 .net "clk", 0 0, v0x9f9bb68_0;  1 drivers
v0x9f9b8e0_0 .var "direction", 0 0;
v0x9f9b948_0 .var "go1", 0 0;
v0x9f9b9b8_0 .var "go2", 0 0;
v0x9f9ba20_0 .net "rst", 0 0, v0x9f9bbe0_0;  1 drivers
v0x9f9bab0_0 .var "s", 3 0;
E_0x9f7ba30 .event posedge, v0x9f9ba20_0, v0x9f7ba58_0;
    .scope S_0x9f7b918;
T_0 ;
    %wait E_0x9f7ba30;
    %load/vec4 v0x9f9ba20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x9f9bab0_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x9f9b8e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x9f9b948_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x9f9b9b8_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x9f9b948_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x9f9b8e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x9f9bab0_0;
    %subi 1, 0, 4;
    %store/vec4 v0x9f9bab0_0, 0, 4;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x9f9bab0_0;
    %addi 1, 0, 4;
    %store/vec4 v0x9f9bab0_0, 0, 4;
T_0.5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x9f9b9b8_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x9f9b948_0, 0, 1;
T_0.3 ;
    %load/vec4 v0x9f9bab0_0;
    %pushi/vec4 15, 0, 4;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x9f9bab0_0;
    %pushi/vec4 0, 0, 4;
    %cmp/e;
    %flag_get/vec4 4;
    %or;
    %load/vec4 v0x9f9b9b8_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %load/vec4 v0x9f9b8e0_0;
    %inv;
    %store/vec4 v0x9f9b8e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x9f9b948_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x9f9b9b8_0, 0, 1;
T_0.6 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x9f9f498;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x9f9bb68_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x9f9bbe0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x9f9bbe0_0, 0, 1;
    %vpi_call/w 2 12 "$display", v0x9f9bc58_0 {0 0 0};
    %pushi/vec4 100, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %load/vec4 v0x9f9bb68_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %vpi_call/w 2 16 "$display", v0x9f9bc58_0 {0 0 0};
T_1.2 ;
    %load/vec4 v0x9f9bb68_0;
    %inv;
    %store/vec4 v0x9f9bb68_0, 0, 1;
    %delay 10, 0;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %end;
    .thread T_1;
